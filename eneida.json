[{
        "id": 1,
        "text": "Еней був парубок моторний,"
    },
    {
        "id": 2,
        "text": "І хлопець хоть куди козак,"
    },
    {
        "id": 3,
        "text": "Удавсь на всеє зле проворний,"
    },
    {
        "id": 4,
        "text": "Завзятіший од всіх бурлак."
    },
    {
        "id": 5,
        "text": "Но греки, як спаливши Трою,"
    },
    {
        "id": 6,
        "text": "Зробили з неї скирту гною"
    },
    {
        "id": 7,
        "text": "Він, взявши торбу, тягу дав;"
    },
    {
        "id": 8,
        "text": "Забравши деяких троянців,"
    },
    {
        "id": 9,
        "text": "Осмалених, як гиря, ланців,"
    },
    {
        "id": 10,
        "text": "П'ятами з Трої накивав."
    },
    {
        "id": 11,
        "text": "Він, швидко поробивши човни,"
    },
    {
        "id": 12,
        "text": "На синє море поспускав,"
    },
    {
        "id": 13,
        "text": "Троянців насажавши повні,"
    },
    {
        "id": 14,
        "text": "І куди очі почухрав."
    },
    {
        "id": 15,
        "text": "Но зла Юнона, суча дочка,"
    },
    {
        "id": 16,
        "text": "Розкудкудакалась, як квочка, — "
    },
    {
        "id": 17,
        "text": "Енея не любила — страх;"
    },
    {
        "id": 18,
        "text": "Давно уже вона хотіла,"
    },
    {
        "id": 19,
        "text": "Його щоб душка полетіла"
    },
    {
        "id": 20,
        "text": "К чортам і щоб і дух не пах."
    }
]